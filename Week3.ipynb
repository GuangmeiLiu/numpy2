#Q3
import numpy as np
def inner(y,x):
a=np.ones(y*x).reshape(y,x)
a[1:y-1,1:x-1]=0
return a

#Q4
import numpy as np
angle=15*np.arange(7)
print(angle)
sine=np.sin(angle)
print(sine)
cosine=np.cos(angle)
print(cosine)
tangent=np.tan(angle)
print(tangent)
arcsine=np.arcsin(sine)*180/np.pi
print(arcsine)
arccosine=np.arccos(cosine)*180/np.pi
print(arccosine)
arctangent=np.arctan(tangent)*180/np.pi
print(arctangent)

